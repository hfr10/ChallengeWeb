{
    "name": "b2-dev/football-ecommerce",
    "description": "Site e-commerce de produits de football - Projet B2 Dev Web",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23"
    },
    "scripts": {
        "start": "php -S localhost:8000 -t public",
        "migrate": "php database/migrate.php",
        "seed": "php database/seed.php"
    },
    "config": {
        "optimize-autoloader": true
    }
}
